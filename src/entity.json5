{
  "repository": {
    "entity": {
      "patterns": [
        {
          "match": "([a-zA-RT-Z][a-zA-Z_]+)(?=[(])",
          "name": "entity.name.function.swift",
        },
        {
          "match": "\\b(func)\\s+([^\\t\\n\\x20\\x28]+)",
          "captures": {
            "1": {"name": "storage.type.function.swift"},
            "2": {"name": "entity.name.function.swift"},
          },
        },
      ]
    }
  }
}
